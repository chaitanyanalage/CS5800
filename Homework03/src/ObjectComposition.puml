@startuml

' Define instances of Folder and File after the 'app' folder deletion

object Folder {
  name = "php_demo1"
  files = []
  subfolders = ["Source Files", " Include Path", "Remote Files"]
}

object SubFolder1 {
  name = "Source Files"
  files = []
  subfolders = [".phalcon", "cache", "public"]
}

object SubFolder1_1 {
  name = ".phalcon"
  files = []
  subfolders = []
}

object SubFolder1_3 {
  name = "cache"
  files = []
  subfolders = []
}

object SubFolder1_4 {
  name = "public"
  files = [".htaccess", ".htrouter.php", "index.html"]
  subfolders = []
}

object File1_4_1 {
  name = ".htaccess"
}

object File1_4_2 {
  name = ".htrouter.php"
}

object File1_4_3 {
  name = "index.html"
}

object SubFolder2 {
  name = "Include Path"
  files = []
  subfolders = []
}

object SubFolder3 {
  name = "Remote Files"
  files = []
  subfolders = []
}

' Define the links representing the composition relationships
Folder  *-- SubFolder1 : subfolder
SubFolder1 *-- SubFolder1_1 : subfolder
SubFolder1 *-- SubFolder1_3 : subfolder
SubFolder1 *-- SubFolder1_4 : subfolder
SubFolder1_4 *-- File1_4_1 : file
SubFolder1_4 *-- File1_4_2 : file
SubFolder1_4 *-- File1_4_3 : file
Folder *-- SubFolder2 : subfolder
Folder *-- SubFolder3 : subfolder

@enduml